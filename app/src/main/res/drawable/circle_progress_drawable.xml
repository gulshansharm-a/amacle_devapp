<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Unfilled part -->
    <item>
        <shape
            android:shape="ring"
            android:useLevel="true">
            <solid android:color="@color/colorNotCompleted" />
        </shape>
    </item>
    <!-- Filled part -->
    <item>
        <shape
            android:innerRadiusRatio="3"
            android:shape="ring"
            android:thicknessRatio="15"
            android:useLevel="true">
            <solid android:color="@color/colorCompleted" />
        </shape>
    </item>
</layer-list>
